<nav class="menu">
  <ul class="list">
    {% for item in items %}
      <li class="list-item">
        <p class="list-title">
          {% if item.url|trim != '#' %}
            <a class="list-titleLink" href="{{ item.url }}" {{ item.target == '_blank' ? 'target="_blank" rel="nofollow noreferrer noopener"' }}>
              {{ item.title|e }}
            </a>
          {% else %}
            {{ item.title|e }}
          {% endif %}
        </p>
      </li>
    {% endfor %}
  </ul>
</nav>
